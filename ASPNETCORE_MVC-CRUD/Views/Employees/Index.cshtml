@model List<ASPNETCORE_CRUD.Models.DomainModels.Employee>
@{
}

<h1>Employee</h1>

<table class="table">
   <thead>
       <tr>
           <th>ID</th>
           <th>Name</th>
           <th>Address</th>
           <th>Position</th>
           <th></th>
       </tr>
   </thead>

   <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Address</td>
                <td>@employee.Position</td>
                <td><a href="Employees/View/@employee.Id">View</a></td>
            </tr>
        }
    </tbody>

</table>